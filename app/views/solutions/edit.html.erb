<main class="wrapper">
  <div class="row columns">
    <div class="title-action">
      <h2 class="title-action__title section-heading">
        <%= t(".edit_solution") %>
      </h2>
    </div>
  </div>
  <div class="row">
    <div id="solutions" class="columns mediumlarge-8 large-9">
      <%= decidim_form_for(@solution, url: solution_path(@solution), html: { multipart: true, class: "form" }) do |form| %>
        <%= render 'form' %>
      <% end %>
    </div>
  </div>
</main>
